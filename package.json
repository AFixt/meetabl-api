{
  "name": "@afixt/meetabl-api",
  "version": "1.0.0",
  "description": "WCAG 2.2 AA/AAA compliant booking API for the meetabl platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon",
    "dev:simple": "nodemon src/index-simple.js",
    "dev:setup": "node scripts/setup-local-db.js",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:staging": "pm2 start ecosystem.config.js --env staging",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "test": "jest --forceExit",
    "test:unit": "jest \"tests/unit/\" --forceExit",
    "test:integration": "jest \"tests/integration/\" --forceExit",
    "test:models": "jest \"tests/unit/models/\" --forceExit",
    "test:controllers": "jest \"tests/unit/controllers/\" --forceExit",
    "test:services": "jest \"tests/unit/services/\" --forceExit",
    "test:middlewares": "jest \"tests/unit/middlewares/\" --forceExit",
    "test:routes": "jest \"tests/integration/routes/\" --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:detail": "jest --coverage --coverageReporters=text-summary --coverageReporters=text",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:badges": "jest --coverage --coverageReporters=json-summary && jest-coverage-badges --input coverage/coverage-summary.json --output docs/badges",
    "coverage:report": "node scripts/generate-coverage.js",
    "lint:md": "markdownlint \"*.md\" \"docs/**/*.md\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint": "npm run lint:md && npm run lint:js",
    "db:migrate": "node src/db/migrate.js",
    "db:migrate:dev": "NODE_ENV=development npm run db:migrate",
    "db:migrate:test": "NODE_ENV=test npm run db:migrate",
    "db:migrate:prod": "NODE_ENV=production npm run db:migrate",
    "db:seed": "node src/db/seed.js",
    "docs:swagger": "swagger-ui-express serve ./src/docs/api/swagger.json",
    "security:check": "npm audit && snyk test",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "logs:setup": "mkdir -p logs",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:prod": "serverless deploy --stage prod",
    "sls:offline": "serverless offline",
    "sls:remove": "serverless remove"
  },
  "keywords": [
    "accessibility",
    "booking",
    "calendar",
    "wcag",
    "appointment"
  ],
  "author": "meetabl Team",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.507.0",
    "@aws-sdk/s3-request-presigner": "^3.507.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "bullmq": "^5.53.2",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "googleapis": "^129.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "nodemailer-express-handlebars": "^6.1.0",
    "pm2": "^6.0.8",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "serverless-http": "^3.2.0",
    "stripe": "^18.2.1",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.0.0",
    "markdownlint-cli": "^0.44.0",
    "nodemon": "^3.0.2",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.7.0",
    "snyk": "^1.1243.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=22"
  }
}
